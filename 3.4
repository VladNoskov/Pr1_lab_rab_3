# Pr1_lab_rab_3
<!DOCTYPE html>
<html>
<head>
<script src="http://code.jquery.com/jquery-latest.js"></script>
<script>	 		
	$(document).ready(function () {
		$("button").click(function () {
		var i=0;
		do{
		$('#div1').delay(400);
		$('#div2').delay(400);
		$('#div3').delay(400);
		$('#div4').delay(400);
		$('#div5').delay(400);
		if(i==0){
		$("#div1").fadeIn(2000);}
		i++;		
		if(i==4){
		$("#div2").fadeIn(2000);}
		if(i==8){
		$("#div3").fadeIn(2000);}
		if(i==12){
		$("#div4").fadeIn(2000);}
		if(i==16){
		$("#div5").fadeIn(2000);}
		if(i==20){
		$("#div5").fadeOut(2000);}
		if(i==24){
		$("#div4").fadeOut(2000);}
		if(i==28){
		$("#div3").fadeOut(2000);}
		if(i==32){
		$("#div2").fadeOut(2000);}
		if(i==36){
		$("#div1").fadeOut(2000);
		i=0;}
}
		while(i<37)
		});
});
</script>
</head>
<body>
<div id="div1" style="width:40px;height:40px;float:left;display:none;background-color:black;"></div>
<div id="div2" style="width:40px;height:40px;float:left;display:none;background-color:yellow;"></div>
<div id="div3" style="width:40px;height:40px;float:left;display:none;background-color:red;"></div>
<div id="div4" style="width:40px;height:40px;float:left;display:none;background-color:blue;"></div>
<div id="div5" style="width:40px;height:40px;float:left;display:none;background-color:brown;"></div><br><br>
</body>
<br><br>
<button>Запуск</button>
</html>
